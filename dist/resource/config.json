{
  "basePath": "dist/resource",
  "savePath": "save",
  "main": "m01.end",
  "debug": false,
  "@auto": false,
  "@autoSpeed": 1000,
  "@textSpeed": 20,
  "text": {
   "path": "scenario",
   "styles": "message-1"
   },
  "image": {
   "path": "image"
   },
  "audio": {
   "bgm": {
    "loop": true,
    "loopStart": 0,
    "loopEnd": 0,
    "playbackRate": 1,
    "amp": "BGM"
    },
   "se": {
    "loop": false,
    "loopStart": 0,
    "loopEnd": 0,
    "playbackRate": 1,
    "amp": "SE"
    }
   },
  "audioNode": {
   "Master": {
    "type": "gain",
    "to": null
    },
   "BGM": {
    "type": "gain",
    "to": "Master"
    },
   "SE": {
    "type": "gain",
    "to": "Master"
    }
   },
  "@volume": {
   "@Master": 100,
   "@BGM": 50,
   "@SE": 50
   }
  }
